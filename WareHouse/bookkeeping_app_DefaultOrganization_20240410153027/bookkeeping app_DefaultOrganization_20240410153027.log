            # Write each record as a new line
            for record in self.income_expense_records:
                line = f"{record['Type']},{record['Amount']},{record['Date']},{record['Notes']}\n"
                file.write(line)
        messagebox.showinfo("Export Data", f"Data exported to {file_name}")
